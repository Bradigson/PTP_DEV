        <th>
            @Html.DisplayNameFor(model => model.TipoComprobante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecuencialInicial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecuenciaDgii)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaVencimiento)
        </th>
  
        <th>
            @Html.DisplayNameFor(model => model.FechaModificacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaCreacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Borrado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Serie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoComprobante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecuencialInicial)
