@model IEnumerable<DataLayer.Models.TipoPago>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        <th>Opciones</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
     
        <td>
            @Html.ActionLink("Edit", "EditarTipoPago", new { id=item.Id }) |
         
            @Html.ActionLink("Delete", "EliminarTp", new { id=item.Id })
        </td>
    </tr>
}

</table>
